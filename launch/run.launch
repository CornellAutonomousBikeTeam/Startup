<launch>
    <node 
        pkg="rosserial_python" 
        name="arduino" 
        type="serial_node.py"
        args="/dev/ttyACM0" 
    />
    
    <node
        pkg="bike"
        name="map"
        type="map_node.py"
    />
    
    <node
        pkg="bike"
        name="nav"
        type="navigation_node.py"
    />
   
    <node pkg="rosbag" type="record" name="recording" args = "-O /home/pi/ros_ws/src/bike/bagfiles/$(arg time) /bike_state /gps /nav_instr /paths" />
</launch>

